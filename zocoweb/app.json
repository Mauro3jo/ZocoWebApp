{
  "expo": {
    "name": "zocoweb",
    "slug": "zocoweb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/Z-icono.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "splash": {
      "backgroundColor": "#000000"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mauro3jo.zocoweb",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/Z-icono.png",
        "backgroundColor": "#000000"
      },
      "package": "com.mauro3jo.zocoweb"
    },

    "androidStatusBar": {
      "backgroundColor": "#F4F6FA",
      "barStyle": "dark-content",
      "translucent": false
    },

    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content",
      "visible": "visible"
    },

    "plugins": [
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "#000000",
          "barStyle": "light",
          "behavior": "inset-swipe",
          "position": "relative"
        }
      ]
    ],

    "web": {
      "favicon": "./assets/img/Z-icono.png"
    },

    "extra": {
      "eas": {
        "projectId": "ccb04cb9-db3e-4d4e-8ac2-b495000aaae7"
      }
    },

    "owner": "mauro3jo"
  }
}
