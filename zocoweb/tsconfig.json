{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@services/*": ["src/services/*"],
      "@context/*": ["src/context/*"],
      "@components/*": ["src/components/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "index.ts", "index.tsx"],
  "exclude": ["node_modules", "dist", "build"]
}
